{
  "name": "@tshirt/react-addressfinder",
  "version": "0.2.4",
  "description": "React address lookup component using AddressFinder",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "prebuild": "rm -rf dist",
    "build": "tsc && cp ./src/widget.css ./dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "homepage": "https://github.com/prospa-group-oss/react-addressfinder",
  "repository": {
    "type": "git",
    "url": "https://github.com/prospa-group-oss/react-addressfinder.git"
  },
  "keywords": [
    "React",
    "addressfinder",
    "autocomplete"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "author": {
    "name": "Miymiy",
    "email": "pukapp@gmail.com"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.21",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "react": "^16.8.6"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}
